<h1>Add Posts</h1>
<%= form_for :pages, url: pages_path do|f| %>
    <p>
        <%= f.label :username %><br>
        <%= f.text_field :username %>
    </p>

    <p> 
        <%= f.label :password %><br>
        <%= f.text_area :password %>
    </p>

    <p>
        <%=f.submit %>
        </p>
    <% end %>